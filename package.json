    "prism-themes": "^1.9.0",
    "prismjs": "^1.27.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.67.0",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-minify-html-literals": "^1.2.6",
